#include <stdio.h>

int main() {
    double total = 780000.0;
    double g1, g2, g3;

    g1 = total * 0.46;
    g2 = total * 0.32;
    g3 = total - (g1 + g2);

    printf("Primeiro ganhador: R$ %.2lf\n", g1);
    printf("Segundo ganhador: R$ %.2lf\n", g2);
    printf("Terceiro ganhador: R$ %.2lf\n", g3);

    return 0;
}
